			<dependency>
				<groupId>org.apache.poi</groupId>
				<artifactId>poi</artifactId>
				<version>${poi.ver}</version>
			</dependency>
			<dependency>
				<groupId>io.netty</groupId>
				<artifactId>netty</artifactId>
				<version>${netty.ver}</version>
			</dependency>
			<!-- jyaml -->
			<dependency>
				<groupId>org.jyaml</groupId>
