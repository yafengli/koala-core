		<spring.ver>3.2.2.RELEASE</spring.ver>
		<aspectj.ver>1.7.2</aspectj.ver>
		<hibernate-validator.ver>5.0.0.Final</hibernate-validator.ver>
		<hibernate-entitymanager.ver>4.2.0.Final</hibernate-entitymanager.ver>
		<mybatis.ver>3.2.2</mybatis.ver>
		<mybatis-spring.ver>1.2.0</mybatis-spring.ver>
		<slf4j.ver>1.7.4</slf4j.ver>        
		<freemarker.ver>2.3.19</freemarker.ver>
		<h2.ver>1.3.171</h2.ver>
		<postgres.ver>9.1-901-1.jdbc4</postgres.ver>
		<commons-lang.ver>2.6</commons-lang.ver>
		<commons-fileupload.ver>1.3</commons-fileupload.ver>
		<commons-net.ver>3.2</commons-net.ver>
		<commons-io.ver>2.4</commons-io.ver>		
		<dbcp.ver>1.4</dbcp.ver>
		<poi.ver>3.9</poi.ver>
		<netty.ver>3.6.5.Final</netty.ver>
		<jyaml.ver>1.3.1</jyaml.ver>
		<xmemcached.ver>1.4.1</xmemcached.ver>
		<jackson.ver>1.9.12</jackson.ver>
		<hadoop.ver>1.1.2</hadoop.ver>
		<jta.ver>1.1</jta.ver>
