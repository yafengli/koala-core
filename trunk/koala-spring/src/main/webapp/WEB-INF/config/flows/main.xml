<?xml version="1.1" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow
        http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd"
	start-state="one">
	<view-state id="one" view="one" redirect="true" popup="true">
		<transition on="totwo" to="two" />
	</view-state>
	<view-state id="two" view="two">
		<transition on="toend" to="end" />
		<transition on="toone" to="one" />
		<transition on="tosub" to="tosub" />
	</view-state>
	<subflow-state id="tosub" subflow="sub">
		<transition on="sube_1" to="end" />
		<transition on="sube_2" to="two" />
	</subflow-state>
	<end-state id="end" view="hello" />
</flow>
        