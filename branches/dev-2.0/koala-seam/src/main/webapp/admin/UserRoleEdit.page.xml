<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd"
      no-conversation-view-id="/admin/UserRoleList.xhtml"
      login-required="false">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{userRoleHome.wire}"/>

   <param name="userRoleFrom"/>
   <param name="userRoleAid" value="#{userRoleHome.userRolePk.aid}"/>
   <param name="userRoleRid" value="#{userRoleHome.userRolePk.rid}"/>
   <param name="accountFrom"/>
   <param name="accountAid" value="#{accountHome.accountAid}"/>

   <param name="roleFrom"/>
   <param name="roleRid" value="#{roleHome.roleRid}"/>



   <navigation from-action="#{userRoleHome.persist}">
      <rule>
         <end-conversation/>
         <redirect view-id="/admin/UserRole.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{userRoleHome.update}">
      <rule>
         <end-conversation/>
         <redirect view-id="/admin/UserRole.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{userRoleHome.remove}">
      <rule>
         <end-conversation/>
         <redirect view-id="/admin/UserRoleList.xhtml"/>
      </rule>
   </navigation>

</page>
